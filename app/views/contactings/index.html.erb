<h1>Contact Us #Index</h1>
<div class="pv4">
  <%= render 'form', contacting: @contacting %>
</div>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Query</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactings.each do |contacting| %>
      <tr>
        <td><%= contacting.name %></td>
        <td><%= contacting.email %></td>
        <td><%= contacting.query %></td>
      </tr>
    <% end %>
  </tbody>
</table>